{
    "name": "eonx-com/easy-cfhighlander",
    "description": "CLI tool to generate Cfhighlander templates",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "eonx-com/utils": "^1.0",
        "twig/twig": "^2.8",
        "symfony/filesystem": "^4.2",
        "symplify/package-builder": "^7.2",
        "symplify/set-config-resolver": "^7.2",
        "symplify/autowire-array-parameter": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "symfony/symfony": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyCfhighlander\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyCfhighlander\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/easy-cfhighlander"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev"
        }
    }
}
