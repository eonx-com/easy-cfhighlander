EnvironmentType:
    development:
        MaxNatGateways: 1
        SingleNatGateway: true
        CacheInstanceType: cache.t2.micro
        BastionInstanceType: t2.micro
        CacheClusters: 1
        RedisCluster: false
        RedisEndpointType: PrimaryEndPoint.Address
        WriterInstanceType: db.t2.small
        ReaderInstanceType: db.t2.small
        SnapshotRetentionLimit: 0
        ReplicasPerNodeGroup: 0
        EnableReader: false
    production:
        MaxNatGateways: 3
        BastionInstanceType: t2.micro
        SingleNatGateway: false
        RDSConnectordbMultiAZ: true
        CacheInstanceType: cache.t2.medium
        CacheClusters: 2
        RedisCluster: true
        RedisEndpointType: ConfigurationEndPoint.Address
        WriterInstanceType: db.t2.small
        ReaderInstanceType: db.t2.small
        SnapshotRetentionLimit: 0
        ReplicasPerNodeGroup: 0
        EnableReader: true

